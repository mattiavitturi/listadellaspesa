<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/list_item_gradient">
        <shape>
            <gradient
                android:id="@+id/test"
                android:startColor="#E95E1E"
                android:endColor="#E91E63"
                android:angle="225" />
            <corners
                android:radius="6dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />

        </shape>
    </item>
    <item android:id="@+id/list_item_icon">
        <bitmap
            android:src="@android:drawable/ic_popup_disk_full"
            android:alpha="200" />

    </item>
</layer-list>